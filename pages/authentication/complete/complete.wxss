.content {
    padding: 10rpx 42rpx;
}

.info-item {
    padding-top: 40rpx;
}

.item-label {
    font-size: 30rpx;
    color: rgba(51, 51, 51, 1);
    line-height: 42rpx;
    margin-bottom: 14rpx;
}

.label-desc {
    font-size: 26rpx;
    color: #999;
}

.item-enter {
    height: 80rpx;
    background: rgba(245, 245, 245, 1);
    border-radius: 8rpx;
    font-size: 28rpx;
    color: rgba(153, 153, 153, 1);
    padding-left: 28rpx;
}

.holder-class {
    color: rgba(153, 153, 153, 1);
}

.protocol-list {
    padding-top: 40rpx;
}

.protocol-item {
    display: inline-block;
    font-size: 28rpx;
    color: rgba(254, 127, 0, 1);
    line-height: 40rpx;
    margin-right: 20rpx;
}

.protocol-item:last-child {
    margin-right: 0;
}
